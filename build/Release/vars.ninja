inc = -I ./include %WLIBPATH64% -I C:/Libraries/glew-2.1.0/include -I C:/Libraries/openssl/Static/Release/include
objDir = ./bin/Release/obj
compiler = cmd /c clang++ 
CXXFLAGS = -std=c++17 -O3 -mavx2
